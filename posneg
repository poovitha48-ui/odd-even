import java.util.Scanner;
public class posneg{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
if(num>0)
System.out.print("positive");
else if(num==0)
System.out.print("zero");
else
System.out.print("negative");
    }
}




OUTPUT:
15
positive
0
zero
-15
negative
