import java.util.Scanner;
public class divnum{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        if(num%3==0 &&num%5==0)
        System.out.print("yes");
        else
        System.out.print("no");
        }
        }




OUTPUT:
  15
  yes
  18
  no
